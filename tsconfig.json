{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es2016", "DOM"],
    "types": ["cypress", "node"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "NodeNext"
  },
  "include": ["**/*.ts"]
}
