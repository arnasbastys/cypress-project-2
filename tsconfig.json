{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es2016", "DOM"],
    "types": ["cypress", "node"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "baseUrl": ".",
    "moduleResolution": "NodeNext",
    "skipLibCheck": true,
    "paths": {
      "@pom": ["cypress/support/pom/index"],
      "@constants": ["./cypress/support/constants"]
    }
  },
  "include": ["**/*.ts"]
}
